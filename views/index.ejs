<%include partials/header.ejs%>
    <h1>Index page</h1>
    <%blogs.forEach(function(blog){%>
    <div>

        <div>
            <h2>
                <%= blog.title %>
            </h2>
        </div>
    <div><img src=<%=blog.image %> alt="Image for the blog" style="width:300px"></div>
        <div>
            <%= blog.body %>
        </div>
        <div>
            <%= blog.created %>
        </div>

    </div>
    <% }); %>
<% include partials/footer.ejs %>